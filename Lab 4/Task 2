#include <iostream>
using namespace std;

int main()
{
    int array[10];
    for (int i = 0;i < 10;i++)
    {
        cout << "Enter a number to input in the array : " << endl;
        cin >> array[i];
    }
    cout << "Unsorted array : ";
    for (int i = 0;i < 10;i++)
    {
        cout << array[i] << ",";
    }
    cout << endl;
    
    for (int i = 0;i < 9;i++)
    {
        for (int j = 0;j < 9;j++)
        {
            if (array[j] > array[j+1])
            {
                int temp = array[j];
                array[j] = array[j+1];
                array[j+1] = temp;
            }
        }
    }
    cout << "Sorted array : ";
    for (int i = 0;i < 10;i++)
    {
        cout << array[i] << ",";
    }
    cout << endl;
}
